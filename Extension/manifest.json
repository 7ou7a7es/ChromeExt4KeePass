{
  "name": "ChromeExt4KeePass",
  "version": "1.0",
  "description": "Very simple change tab title with URL for KeePass Auto-Type",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
